        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1",
        "symfony/dotenv": "^4.1"
    },
    "license": "MIT",
    "authors": [
